	<queryString>
		<![CDATA[SELECT
detail.id,
customer.companame_en,
customer.companame_th,
customer.cusname,
customer.`position`,
customer.mail,
customer.address,
customer.tel08,
customer.tel02,
customer.fax,
project.proid,
project.proname,
status.statusid,
status.statusname,
detail.date,
employee.username,
employee.empname
FROM
customer
Inner Join detail ON customer.cusid = detail.cusid
Inner Join project ON project.proid = detail.proid
Inner Join status ON status.statusid = detail.statusid
Inner Join employee ON employee.username = detail.username
where employee.username like $P{username}
order by project.proid]]>
	</queryString>
